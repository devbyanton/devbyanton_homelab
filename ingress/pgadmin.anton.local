apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  namespace: pgadmin
  name: pgadmin-ingress
spec:
  rules:
  - host: pgadmin.anton.local
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: pgadmin
            port:
              number: 80
